    "description": "台灣藍新(智付通) Laravel API",
    "license": "MIT",
    "authors": [
        {
            "name": "mingjshk",
            "email": "mingj85415@gmail.com"
        }
    ],
    "homepage": "https://github.com/MingJSHK/laravel-newebpay",
    "require": {
        "php": ">=5.6",
        "guzzlehttp/guzzle": ">=6.3",
        "illuminate/config": ">=5.1",
        "illuminate/contracts": ">=5.1",
        "illuminate/log": ">=5.1",
        "illuminate/support": ">=5.1",
        "nesbot/carbon": ">=1.0"
    },
    "autoload": {
        "psr-4":{
